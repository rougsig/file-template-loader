{
  "name": "Route",
  "templates": [
    {
      "template": "Key.kt.ft",
      "fileName": "${ROUTE_NAME}Key"
    },
    {
      "template": "Screen.kt.ft",
      "fileName": "${ROUTE_NAME}Screen"
    }
  ],
  "injectors": [
    {
      "text": "\nis ${APP_ROUTE_SIMPLE_NAME}.${ROUTE_NAME} -> ${SCREEN_SIMPLE_NAME}()",
      "className": "${SCREEN_FACTORY_CLASS_NAME}",
      "selector": "INVOKE WHEN WHEN_ENTRY"
    },
    {
      "text": "\nimport ${KEY_CLASS_NAME}",
      "className": "${SCREEN_FACTORY_CLASS_NAME}",
      "selector": "IMPORT_DIRECTIVE"
    },
    {
      "text": ",\n':module-name'",
      "pathName": "settings.gradle",
      "selector": "Literal"
    }
  ]
}
